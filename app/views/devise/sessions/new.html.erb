<h2>Welome! please Log in if you have an account already else Sign up ?</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name),class:'login-form') do |f| %>
 <div class="form-group">
  <div class="field-input">
    <%= f.email_field :email, autofocus: true, placeholder:'useremail', autocomplete: "email" %>
  </div>

  <div class="field-input">
    <%= f.password_field :password,  placeholder:'Password',autocomplete: "current-password" %>
  </div>

  <div class="submit-actions" >
    <%= f.submit "Log in" %>
  </div>
  </div>
<% end %>

<%= render "devise/shared/links" %>